+  poly_write(stderr, got);
+  fputs("\nexp:\n", stderr);
+  poly_write(stderr, exp);
+  fputs("\n", stderr);
+
+  // print differences
+  size_t num = 0;
+  for (size_t i = 0; i < 256; i++) {
+    if (got->cs[i] != exp->cs[i]) {
+      const char *p = (num == 0) ? "diffs:" : ",";
+      fprintf(stderr, "%s %zu (got %d exp %d)", p, i, got->cs[i], exp->cs[i]);
+      num++;
+    }
+  }
+  fputs("\n", stderr);
+}
