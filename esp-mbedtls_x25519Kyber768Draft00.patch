+            status = psa_raw_key_agreement(
+                alg, handshake->xxdh_psa_privkey,
+                handshake->xxdh_psa_peerkey, 32,
+                shared_secret, 32, &shared_secret_len);
+			memcpy(&shared_secret[32], tmp_kem768_ss, 32);
