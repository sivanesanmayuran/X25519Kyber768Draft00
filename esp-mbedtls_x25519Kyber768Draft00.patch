-    /* check for expected value */ \
-    for (size_t i = 0; i < K*(K + 2) + 1; i++) { \
-      if (memcmp(got + i, exp + i, sizeof(poly_t))) { \
-        /* build test name */ \
-        char name[32] = { 0 }; \
-        snprintf(name, sizeof(name), "test %zu, poly %zu", t, i); \
-    \
-        /* fail test */ \
-        fail_poly_test(func, name, got + i, exp + i); \
