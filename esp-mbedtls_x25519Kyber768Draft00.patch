+    // decode u12s values
+    uint16x8x2_t s;
+    s.val[0] = (vmovl_u8(vtbl3_u8(d, C0_LO_IDS) & LO_MASK) >> LO_SHIFT) | (vmovl_u8(vtbl3_u8(d, C0_HI_IDS) & HI_MASK) << HI_SHIFT);
