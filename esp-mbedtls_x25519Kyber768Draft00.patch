+          a16 = _mm512_maskz_set1_epi64(0xf0, 0x8000000000000000ULL),
+          a17 = _mm512_setzero_si512(),
+          a18 = _mm512_setzero_si512(),
+          a19 = _mm512_setzero_si512();
