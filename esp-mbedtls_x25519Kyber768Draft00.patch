+  // orig: uint32_t lo_shift_vals[16] ALIGN(64) = { 0, 5, 2, 7, 4, 1, 6, 3, 0, 5, 2, 7, 4, 1, 6, 3 },
+  static const __m512i LO_SHIFTS = {
+    0x0000000500000000ULL, 0x0000000700000002ULL, 0x0000000100000004ULL, 0x0000000300000006ULL,
+    0x0000000500000000ULL, 0x0000000700000002ULL, 0x0000000100000004ULL, 0x0000000300000006ULL,
