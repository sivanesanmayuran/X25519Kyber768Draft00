-  vec4_ntt(r); // r = NTT(r)
-
-  // u = Ar + e1
-  poly_t u[PKE1024_K] = { 0 };
-  mat4_mul(u, a, r);  // u = (A*r)
-  vec4_inv_ntt(u);    // u = InvNTT(u)
