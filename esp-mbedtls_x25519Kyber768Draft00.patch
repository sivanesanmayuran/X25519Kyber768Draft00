+  // - lanes 0: rho for e2
+  // - other lanes: unused
+  a0 = _mm512_maskz_set1_epi64(1, sp[0]);
+  a1 = _mm512_maskz_set1_epi64(1, sp[1]);
+  a2 = _mm512_maskz_set1_epi64(1, sp[2]);
+  a3 = _mm512_maskz_set1_epi64(1, sp[3]);
