+    if (memcmp(got, PKE512_DECRYPT_TESTS[i].exp, sizeof(got))) {
+      fail_test(__func__, PKE512_DECRYPT_TESTS[i].name, got, sizeof(got), PKE512_DECRYPT_TESTS[i].exp, sizeof(got));
+    }
+  }
