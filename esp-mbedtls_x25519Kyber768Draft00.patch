+      TRANSPOSE_8(a0, a1, a2, a3, a4, a5, a6, a7);
+
+      // sample Ahat polynomial coefficients
+      SAMPLE_32_COEFS(t0, 0); // Ahat[0, 0]
+      SAMPLE_32_COEFS(t1, 1); // Ahat[0, 1]
+      SAMPLE_32_COEFS(t2, 2); // Ahat[1, 0]
+      SAMPLE_32_COEFS(t3, 3); // Ahat[1, 1]
