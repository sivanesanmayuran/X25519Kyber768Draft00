+      // sample bytes
+      SAMPLE_CBD3_BYTES(t4, 4, 48); // s[0]
+      SAMPLE_CBD3_BYTES(t5, 5, 48); // s[1]
+      SAMPLE_CBD3_BYTES(t6, 6, 48); // e[0]
+      SAMPLE_CBD3_BYTES(t7, 7, 48); // e[1]
