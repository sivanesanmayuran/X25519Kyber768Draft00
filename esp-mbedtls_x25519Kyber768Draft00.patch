-// define shake iterative context and one-shot functions
-#define DEF_SHAKE_CONTEXT(BITS) \
-  /* init shake context */ \
-  static inline void shake ## BITS ## _init(sha3_xof_t * const xof) { \
-    xof_init(xof); \
-  } \
-  \
