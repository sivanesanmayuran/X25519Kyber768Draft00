+      fail_test(__func__, name, got_ek, sizeof(got_ek), PKE512_KEYGEN_TESTS[i].exp_ek, sizeof(got_ek));
+    }
+
+    // check for expected dk
+    if (memcmp(&got_dk, &(PKE512_KEYGEN_TESTS[i].exp_dk), sizeof(got_dk))) {
+      char name[64];
