+    } \
+  } \
+  static inline void vec ## N ## _encode_11bit_neon(uint8_t out[static 352*N], const poly_t v[static N]) { \
+    for (size_t i = 0; i < N; i++) { \
+      poly_encode_11bit_neon(out + 352*i, v + i); \
