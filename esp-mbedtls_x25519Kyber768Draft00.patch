-#define DEF_HASH(BITS, OUT_LEN) \
-  /* one-shot hash */ \
-  static inline void sha3_ ## BITS(const uint8_t *m, size_t m_len, uint8_t dst[static OUT_LEN]) { \
-    hash_once(m, m_len, dst, OUT_LEN); \
-  }
