+  // - lanes 0-3: rho for a
+  // - lanes 4-7: rho for s and e
+  __m512i a0 = _mm512_setr_epi64(ap[0], ap[0], ap[0], ap[0], sp[0], sp[0], sp[0], sp[0]),
