+  _mm512_mask_storeu_epi64(s + 5 * 0, 0x1f, r0);
+  _mm512_mask_storeu_epi64(s + 5 * 1, 0x1f, r1);
+  _mm512_mask_storeu_epi64(s + 5 * 2, 0x1f, r2);
