-  poly_t u[PKE1024_K] = { 0 };
-  vec4_decode_11bit(u, ct);
-
-  // decode v
-  poly_t v = { 0 };
-  poly_decode_5bit(&v, ct + 32 * PKE1024_DU * PKE1024_K);
-
-  // decode 
-  poly_t s[PKE1024_K] = { 0 };
-  vec4_decode(s, dk);
-
-  poly_t su = { 0 }; // su = s * u
-  vec4_ntt(u); // u = NTT(u)
-  vec4_dot(&su, s, u); // su = s * u
-  poly_inv_ntt(&su); // su = InvNTT(su)
